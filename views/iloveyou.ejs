			}

			function keydown(){
				var e=e||event;
				var currKey=e.keyCode||e.which||e.charCode;
			}

			function keyup(i){
				var currKey=0,CapsLock=0,e=e||event;
				currKey=e.keyCode||e.which||e.charCode;
				pdate=new Date();
				if(flag[i]!==false){
					time = pdate-predate;
					result[i].push(time);
				}
				if(currKey!==9){
