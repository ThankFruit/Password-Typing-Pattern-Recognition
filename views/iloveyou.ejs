					for(var j=0; j<pass.length-1; j++){
						center[j] = center[j]+result[i][j];
					}
				}
				for(var i=0; i<pass.length-1; i++){
					center[i] = center[i]/count;
				}
			}
			
			function checkValid(){
				for(var i=0; i<result.length; i++){
					if(result[i].length === pass.length-1){
						valid[i] = true;
						count = count + 1;
					}
				}
			}
			
			function getUrl(obj){
	          var qstring = "?"
		      for (var k in obj) {
